# Next.js
NEXT_PUBLIC_APP_URL=http://localhost:3000

# Supabase
NEXT_PUBLIC_SUPABASE_URL=your_supabase_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key

# IPFS (Pinata or similar)
PINATA_API_KEY=your_pinata_api_key
PINATA_SECRET_KEY=your_pinata_secret_key

# Memecore Blockchain Configuration
# Private key for deploying tokens (without 0x prefix or with it)
MEMECORE_PRIVATE_KEY=0x1234567890abcdef...

# Network to deploy to (mainnet, insectarium, formicarium)
MEMECORE_NETWORK=insectarium

# Enable/disable automatic token deployment when wikis are created
ENABLE_TOKEN_DEPLOYMENT=true

# MRC-20 Token Contract Bytecode (get from compilation)
# Run: cd contracts && npx hardhat compile
# Then copy bytecode from: contracts/artifacts/contracts/MRC20Token.sol/MRC20Token.json
MRC20_BYTECODE=0x...

# Ethereum/Other Networks (existing)
PRIVATE_KEY=your_private_key
SEPOLIA_RPC_URL=https://sepolia.infura.io/v3/your_key
MAINNET_RPC_URL=https://mainnet.infura.io/v3/your_key
